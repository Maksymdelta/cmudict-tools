@prefix dct:  <http://purl.org/dc/terms/> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix xsd:  <http://www.w3.org/2001/XMLSchema> .

@prefix cmu: <http://reecedunn.co.uk/linguistics/pos/cmu/> .

cmu: a skos:ConceptScheme ;
	dct:title "CMU Pronunciation Dictionary Context Tags"@en ;
	dct:description "This is the context tagset used in the CMU pronunciation dictionary." ;
	skos:notation "cmu" ;
	skos:hasTopConcept cmu:1 ;
	skos:hasTopConcept cmu:2 ;
	skos:hasTopConcept cmu:3 ;
	skos:hasTopConcept cmu:4 ;
	skos:hasTopConcept cmu:5 ;
	skos:hasTopConcept cmu:6 ;
	skos:hasTopConcept cmu:7 ;
	skos:hasTopConcept cmu:8 ;
	skos:hasTopConcept cmu:9 .

cmu:1 a skos:Concept ;
	skos:prefLabel "1"@en ;
	skos:notation "1" ;
	skos:note "Used in air and cmudict-new formatted dictionaries as of svn commit 9094."@en ;
	skos:inScheme cmu: .

cmu:2 a skos:Concept ;
	skos:prefLabel "2"@en ;
	skos:notation "2" ;
	skos:inScheme cmu: .

cmu:3 a skos:Concept ;
	skos:prefLabel "3"@en ;
	skos:notation "3" ;
	skos:inScheme cmu: .

cmu:4 a skos:Concept ;
	skos:prefLabel "4"@en ;
	skos:notation "4" ;
	skos:inScheme cmu: .

cmu:5 a skos:Concept ;
	skos:prefLabel "5"@en ;
	skos:notation "5" ;
	skos:note "First used in cmudict 0.6."@en ;
	skos:inScheme cmu: .

cmu:6 a skos:Concept ;
	skos:prefLabel "6"@en ;
	skos:notation "6" ;
	skos:note "First used in cmudict 0.6."@en ;
	skos:inScheme cmu: .

cmu:7 a skos:Concept ;
	skos:prefLabel "7"@en ;
	skos:notation "7" ;
	skos:note "First used in cmudict 0.6d."@en ;
	skos:inScheme cmu: .

cmu:8 a skos:Concept ;
	skos:prefLabel "8"@en ;
	skos:notation "8" ;
	skos:note "First used in cmudict 0.6d."@en ;
