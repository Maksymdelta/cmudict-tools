	skos:narrowMatch wp39:1 ;
	skos:narrowMatch wp39:2 ;
	skos:narrowMatch wp39:jj ;
	skos:inScheme upenn: .

upenn:jjr a skos:Concept ;
	skos:prefLabel "comparative adjective"@en ;
